<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fh.dao.ShopTypeMapper" >
    <select id="queryShopType" resultType="com.fh.model.ShopType">
        select tid,pid,typeName from shoptype where isValid =1 order by sort_order
    </select>

</mapper>